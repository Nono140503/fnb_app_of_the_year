{
  "expo": {
    "name": "Fnb_App",
    "slug": "Fnb_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo3-removebg-preview.png",
      "resizeMode": "contain",
      "backgroundColor": "#1d61e7"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload videos.",
        "NSCameraUsageDescription": "This app needs access to your camera to take videos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1d61e7"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/Untitled_design.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to your photo library to upload videos.",
          "cameraPermission": "This app needs access to your camera to take videos."
        }
      ]
    ]
  }
}
